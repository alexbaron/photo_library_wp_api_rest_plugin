{
    "name": "alex/photo_library_rest_api",
    "description": "Wordpress plugin to share image to react phototeque app.",
    "type": "project",
    "require": {
        "ksubileau/color-thief-php": "^2.0.2"
    },
    "require-dev": {
        "joachim-n/composer-manifest": "^1.1",
        "squizlabs/php_codesniffer": "^3.13",
        "wp-coding-standards/wpcs": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Alex\\PhotoLibraryRestApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Baron",
            "email": "159906+alexbaron@users.noreply.github.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "joachim-n/composer-manifest": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=phpcs.xml",
        "phpcs:report": "phpcs --standard=phpcs.xml --report=summary",
        "phpcbf": "phpcbf --standard=phpcs.xml",
        "cs:check": "@phpcs:report",
        "cs:fix": "@phpcbf"
    }
}
